{
  "name": "ci-cd-examples",
  "packageManager": "yarn@4.11.0",
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf build && next export && mv ./out ./build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "deploy:prod": "yarn build && vercel ./build --confirm --no-clipboard --token=$VERCEL_TOKEN --prod"
  }
}
